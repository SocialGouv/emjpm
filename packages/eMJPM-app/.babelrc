{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "targets": {
            "browsers": ["last 5 Chrome version", "ie >= 11"]
          }
        }
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": ["@babel/plugin-transform-flow-strip-types"]
    },
    "development": {
      "plugins": ["@babel/plugin-transform-flow-strip-types"]
    },
    "test": {
      "presets": ["env", "react"],
      "plugins": ["@babel/plugin-transform-flow-strip-types"]
    }
  }
}
