{
  "name": "@emjpm/api",
  "version": "25.144.0",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "build": "ncc build src/start.js --source-map -e oracledb -e oracle -e pg-query-stream -e mysql2 -e strong-oracle -e sqlite3 -e mysql -e mariasql -e mssql",
    "dev": "nodemon --watch src --watch $(realpath ../biz) --inspect --exec node --trace-warnings src/start.js",
    "dev:knex-debug-query": "DEBUG=knex:query nodemon src/start.js --inspect",
    "test": "yarn test:unit && yarn test:e2e",
    "test:watch": "yarn test:unit:watch",
    "test:unit": "NODE_ENV=test jest --config ./jest.unit.config.js --maxWorkers=2",
    "test:unit:watch": "NODE_ENV=test npx jest --config ./jest.unit.config.js --maxWorkers=2 --watch",
    "test:e2e": "NODE_ENV=test jest --config ./jest.e2e.config.js --maxWorkers=2",
    "test:e2e:watch": "NODE_ENV=test npx jest --config ./jest.e2e.config.js --maxWorkers=2 --watch",
    "test:e2e:update-snapshot": "NODE_ENV=test npx jest --config ./jest.e2e.config.js --maxWorkers=2 --updateSnapshot",
    "lint:pre-commit": "yarn lint:fix",
    "lint": "eslint __test__ src",
    "lint:fix": "yarn lint --cache --fix",
    "prettier": "prettier --check '**/*.js'",
    "prettier:fix": "prettier --write '**/*.js'",
    "postinstall": "link-module-alias"
  },
  "_moduleAliases": {
    "~": "src"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.7.0",
    "@emjpm/biz": "^25.144.0",
    "@sentry/node": "5.30.0",
    "@vercel/ncc": "^0.29.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.23.0",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-oauth-server": "^2.0.0",
    "express-pino-logger": "^6.0.0",
    "express-rate-limit": "^5.3.0",
    "express-validator": "^6.12.1",
    "fnv-plus": "^1.3.1",
    "generate-password": "^1.6.1",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "knex": "^0.95.10",
    "lightship": "^6.7.2",
    "line-reader": "^0.4.0",
    "link-module-alias": "^1.2.0",
    "lodash.uniq": "^4.5.0",
    "node-7z": "^2.1.2",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.6.3",
    "objection": "^2.2.15",
    "objection-db-errors": "^1.1.2",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.1",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-stub": "^1.1.1",
    "pg": "^8.7.1",
    "pino": "^5.17.0",
    "query-string": "^6.14.1",
    "rand-token": "^0.4.0",
    "rasha": "^1.2.5",
    "stream-chain": "^2.2.4",
    "stream-json": "^1.7.2",
    "tmp": "^0.2.1",
    "xlsx": "^0.17.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@socialgouv/eslint-config-recommended": "^1.85.4",
    "babel-core": "7.0.0-bridge.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^7.32.0",
    "jest": "^27.0.6",
    "jest-environment-knex": "^3.0.7",
    "lint-staged": "^11.1.2",
    "nodemailer-mock": "^1.5.11",
    "nodemon": "^1.19.4",
    "pg": "^8.7.1",
    "pg-query-stream": "^4.2.1",
    "pino-pretty": "^5.1.3",
    "prettier": "^2.3.2",
    "rewiremock": "^3.14.3",
    "supertest": "^6.1.6"
  }
}
