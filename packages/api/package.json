{
  "name": "@emjpm/api",
  "version": "0.0.4",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "build": "ncc build app.js -e oracledb -e oracle -e pg-query-stream -e mysql2 -e strong-oracle -e sqlite3 -e mysql -e mariasql -e mssql",
    "dev": "nodemon app.js",
    "test:mocha": "NODE_ENV=test mocha --timeout 10000 --exit",
    "test": "NODE_ENV=test jest --runInBand --verbose",
    "knex": "knex",
    "lint": "eslint __test__ auth db email routes scripts test app.js knexfile.js",
    "migrate-and-start": "knex migrate:latest && npm start",
    "seeds": "knex seed:run",
    "migrate-seeds-and-start": "knex migrate:latest && npm run seeds && npm start",
    "wait-migrate-and-start": "sleep 10 && knex migrate:latest && npm start",
    "email-relance": "node ./scripts/relance-mandataires-inactifs"
  },
  "dependencies": {
