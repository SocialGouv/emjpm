{
  "name": "@emjpm/api",
  "version": "25.80.0",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "build": "ncc build src/start.js --source-map -e oracledb -e oracle -e pg-query-stream -e mysql2 -e strong-oracle -e sqlite3 -e mysql -e mariasql -e mssql",
    "dev": "nodemon --watch src --watch $(realpath ../biz) --inspect --exec node --trace-warnings src/start.js",
    "dev:knex-debug-query": "DEBUG=knex:query nodemon src/start.js --inspect",
    "test": "yarn test:unit && yarn test:e2e",
    "test:watch": "yarn test:unit:watch",
    "test:unit": "NODE_ENV=test jest --config ./jest.unit.config.js --maxWorkers=2",
    "test:unit:watch": "NODE_ENV=test npx jest --config ./jest.unit.config.js --maxWorkers=2 --watch",
    "test:e2e": "NODE_ENV=test jest --config ./jest.e2e.config.js --maxWorkers=2",
    "test:e2e:watch": "NODE_ENV=test npx jest --config ./jest.e2e.config.js --maxWorkers=2 --watch",
    "lint:pre-commit": "yarn lint:fix",
    "lint": "eslint __test__ src",
    "lint:fix": "yarn lint --cache --fix",
    "prettier": "prettier --check '**/*.js'",
    "prettier:fix": "prettier --write '**/*.js'",
    "postinstall": "yarn postinstall:${NODE_ENV:-development}",
    "postinstall:test": "yarn postinstall:development",
    "postinstall:development": "link-module-alias",
    "postinstall:production": "exit 0"
  },
  "_moduleAliases": {
    "~": "src"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.2.1",
    "@emjpm/biz": "^25.80.0",
    "@sentry/node": "5.27.2",
    "@vercel/ncc": "^0.25.1",
    "axios": "^0.21.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "debug": "^4.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-oauth-server": "^2.0.0",
    "express-pino-logger": "^4.0.0",
    "express-rate-limit": "^5.1.3",
    "express-validator": "^6.6.1",
    "fnv-plus": "^1.3.1",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.0.0",
    "knex": "^0.21.11",
    "link-module-alias": "^1.2.0",
    "lodash.uniq": "^4.5.0",
    "node-7z": "^2.1.2",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.4.14",
    "objection": "^2.2.3",
    "objection-db-errors": "^1.1.2",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.1",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-stub": "^1.1.1",
    "pg": "^8.4.2",
    "pino": "^5.17.0",
    "query-string": "^6.13.6",
    "rand-token": "^0.4.0",
    "rasha": "^1.2.5",
    "tmp": "^0.2.1",
    "xlsx": "^0.16.8",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@socialgouv/eslint-config-recommended": "^1.46.0",
    "babel-core": "7.0.0-bridge.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.12.1",
    "jest": "^24.9.0",
    "jest-environment-knex": "^3.0.5",
    "nodemailer-mock": "^1.5.3",
    "nodemon": "^1.19.4",
    "pg": "^8.4.2",
    "pg-query-stream": "^2.1.2",
    "pino-pretty": "^3.6.1",
    "prettier": "^2.1.2",
    "rewiremock": "^3.14.3",
    "supertest": "^6.0.0"
  }
}
