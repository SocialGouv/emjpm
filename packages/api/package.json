{
  "name": "@emjpm/api",
  "version": "25.60.0",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "build": "ncc build src/start.js --source-map -e oracledb -e oracle -e pg-query-stream -e mysql2 -e strong-oracle -e sqlite3 -e mysql -e mariasql -e mssql",
    "dev": "nodemon src/start.js --inspect",
    "dev:knex-debug-query": "DEBUG=knex:query nodemon src/start.js --inspect",
    "test": "yarn test:unit && yarn test:e2e",
    "test:watch": "yarn test:unit:watch",
    "test:unit": "NODE_ENV=test jest --config ./jest.unit.config.js --maxWorkers=2",
    "test:unit:watch": "NODE_ENV=test npx jest --config ./jest.unit.config.js --maxWorkers=2 --watch",
    "test:e2e": "NODE_ENV=test jest --config ./jest.e2e.config.js --maxWorkers=2",
    "test:e2e:watch": "NODE_ENV=test npx jest --config ./jest.e2e.config.js --maxWorkers=2 --watch",
    "lint:pre-commit": "npm run lint:fix",
    "lint": "eslint __test__ src",
    "lint:fix": "eslint --cache __test__ src --fix",
    "prettier": "prettier --check '**/*.js'",
    "prettier:fix": "prettier --write '**/*.js'"
  },
  "dependencies": {
    "@emjpm/core": "^25.60.0",
    "@emjpm/knex": "^25.60.0",
    "@sentry/node": "5.15.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "date-fns": "^2.15.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-pino-logger": "^4.0.0",
    "express-validator": "^6.6.1",
    "fnv-plus": "^1.3.1",
    "http-errors": "^1.7.3",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "knex": "^0.21.5",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.4.11",
    "objection": "^2.1.6",
    "objection-db-errors": "^1.1.2",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.1",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-stub": "^1.1.1",
    "pino": "^5.17.0",
    "query-string": "^6.12.1",
    "rand-token": "^0.4.0",
    "rasha": "^1.2.5",
    "tmp": "^0.2.1",
    "xlsx": "^0.16.6",
    "yup": "^0.28.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@socialgouv/eslint-config-recommended": "^1.28.0",
    "@zeit/ncc": "^0.22.3",
    "babel-core": "7.0.0-bridge.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest-cli": "^24.9.0",
    "jest-environment-knex": "^1.1.14",
    "nodemailer-mock": "^1.5.3",
    "nodemon": "^1.19.4",
    "pg": "^7.18.2",
    "pg-query-stream": "^2.1.2",
    "pino-pretty": "^3.6.1",
    "prettier": "^2.0.5",
    "rewiremock": "^3.14.3",
    "supertest": "^4.0.2"
  }
}
