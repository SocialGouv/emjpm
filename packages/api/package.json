    "date-fns": "^1.29.0",
    "debug": "^2.6.9",
    "dotenv": "^6.1.0",
    "es6-promise": "^4.2.4",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "http-errors": "^1.7.1",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.3.0",
